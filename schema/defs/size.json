{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://github.com/francescozoccheddu/ae-3d2shape/schema/defs/size.json",
    "$defs": {
        "dimension": {
            "type": "number",
            "minimum": 0
        }
    },
    "type": "object",
    "properties": {
        "width": {
            "$ref": "#/$defs/dimension"
        },
        "height": {
            "$ref": "#/$defs/dimension"
        }
    },
    "required": [
        "width",
        "height"
    ],
    "additionalProperties": false
}